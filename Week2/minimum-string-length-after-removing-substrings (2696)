https://leetcode.com/problems/minimum-string-length-after-removing-substrings/submissions/1889546307/

class Solution {
public:
    int minLength(string s) {
        stack<char> st;
        for(auto c: s){
            if (!st.empty() && ((c == 'B' && st.top() == 'A') || (c == 'D' && st.top() == 'C'))) {
            st.pop();
            }else{
                st.push(c);
            }
        }
        return st.size();
    }
};
